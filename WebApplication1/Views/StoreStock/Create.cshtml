@using WebApplication1.ViewModel
@model StoreStocksModel

<h2>Create</h2>
<div>
    <button class="btn btn-danger" onclick="location.href = '@Url.Action("StoreStock", "StoreStock")'" />Back
</div>
@using (Html.BeginForm("Create", "StoreStock", FormMethod.Post))
{
    @Html.ValidationSummary()
<table class="center">
    <tr>
        <td> @Html.HiddenFor(m => m.StoreId)</td>
    </tr>
    <tr>
        <td>Product ID: </td>
        <td>@Html.TextBoxFor(m => m.ProductId)</td>

    </tr>
    <tr>
        <td>Quantity: </td>
        <td>@Html.TextBoxFor(m => m.quantity)</td>
    </tr>
    @*<tr>
        <td colspan="2"><input type="submit" value="Submit Form" /></td>
    </tr>*@
    

</table>
<div>
    <p align="center">
        <button id="btnsubmit" type="submit" class="btn btn-success" value="Add">SUBMIT</button>
    </p>
</div>

}
